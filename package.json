{
  "name": "gcc_website",
  "version": "0.0.1",
  "description": "Website for Grace Covenant Church",
  "main": "server/app.js",
  "scripts": {
    "start": "concurrently \"npm run dev-client\" \"npm run dev-server\" \"npm run dev-admin-client\" \"npm run dev-admin-server\" --kill-others",
    "dev-server": "cross-env SERVER_PORT=8080 CLIENT_PORT=3000 nodemon src/server/app.js --exec babel-node --presets=env,react --plugins=dynamic-import-node",
    "dev-client": "cross-env SERVER_PORT=8080 CLIENT_PORT=3000 webpack-dev-server --config src/webpack.config.js",
    "dev-admin-server": "cross-env SERVER_PORT=8081 CLIENT_PORT=3001 nodemon src/admin_server/app.js --exec babel-node --presets=env,react --plugins=dynamic-import-node",
    "dev-admin-client": "cross-env SERVER_PORT=8081 CLIENT_PORT=3001 webpack-dev-server --config src/webpack.admin.js",
    "webpack": "cross-env NODE_ENV=production webpack --config src/webpack.config.js -p",
    "webpack-admin": "cross-env NODE_ENV=production webpack --config src/webpack.admin.js -p",
    "serve": "cross-env NODE_ENV=production node build/server/app.js",
    "serve-admin": "cross-env NODE_ENV=production node build/admin_server/app.js",
    "babel": "cross-env NODE_ENV=production babel src -d build --copy-files --presets=env,react --plugins=dynamic-import-node"
  },
  "author": "gccdev",
  "license": "ISC",
  "dependencies": {
    "compression": "^1.7.0",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^1.3.2",
    "cross-env": "^5.0.5",
    "express": "^4.15.5",
    "forever": "^0.15.3",
    "isomorphic-fetch": "^2.2.1",
    "mongodb": "^2.2.31",
    "mongoose": "^4.11.13",
    "morgan": "^1.9.0",
    "nconf": "^0.8.4",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "url-join": "^2.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-dynamic-import-node": "^1.1.0",
    "babel-plugin-dynamic-import-webpack": "^1.0.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.0",
    "http-proxy-middleware": "^0.17.4",
    "nodemon": "^1.12.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.8.2"
  }
}
